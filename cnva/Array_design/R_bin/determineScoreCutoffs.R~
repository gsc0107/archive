
#Open a file containing the simfold and pairfold scores for all region and negative control probes
datadir = "/home/malachig/MR_Chip_Design/stats"
dir(datadir)
setwd(datadir)

scores = read.table("all_scores.txt", header=T, quote="", sep="\t", comment.char="", na.strings = "na")

quantile(scores[which(scores[,"Probe_length"]==44), "SimFold_score"], probs = seq(0, 1, 0.05), na.rm = TRUE)
